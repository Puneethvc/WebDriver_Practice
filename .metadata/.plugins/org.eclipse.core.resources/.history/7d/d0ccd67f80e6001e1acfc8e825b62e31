package Arrays;

public class Cherie {

	public static void main(String[] args) {


		String s ="bnb";
		String rev ="";
		for(int i=s.length()-1;i>=0;i--) {   // B
			rev=rev+s.charAt(i);
		}
		if(s == rev) {
			System.out.println("given string is palindrome --"+ rev);
		}else
			System.out.println("not plaindrome --");

	}

}
