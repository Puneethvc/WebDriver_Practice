package Arrays;

public class Palindrome {

	public static void main(String[] args) {
		//String
		
		String s = "Amma";
		String rev="";
		for(int i=s.length()-1;i>0;i--)
		{
			rev= rev+s.charAt(i);
			
		}
		if(s.equals(rev)) {
			System.out.println(" palindrome String"+rev);
		}
		System.out.println("hi");
	}

}
